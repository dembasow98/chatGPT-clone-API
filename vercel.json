{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/nodemon"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "devDependencies": {
        "@vercel/nodemon": "1.9.0"
    },
    "installCommand": "pnpm install"
}